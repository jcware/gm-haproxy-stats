{
  "name": "gm-haproxy-stats",
  "version": "1.0.0",
  "description": "Verify HAPROXY stats awesomely.",
  "main": "index.js",
  "scripts": {
    "clear": "rimraf lib",
    "build": "yarn clear && ./node_modules/.bin/babel --out-dir lib src",
    "build:watch": "yarn build -- --watch",
    "lint": "./node_modules/.bin/eslint src/*.js",
    "lint:watch":"gulp",
    "lint:fix": "yarn lint -- --fix",
    "prepush": "yarn lint",
    "test": "./node_modules/.bin/mocha tests/**/*.test.js --require babel-register",
    "test:watch": "yarn test -- --watch",
    "test:coverage": "nyc npm test"
  },
  "nyc": {
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jcware/gm-haproxy-stats.git"
  },
  "keywords": [
    "js",
    "haproxy",
    "library"
  ],
  "author": "Julio Cousteau <juliocousteau@gmail.com> (https://jcware.com.br/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jcware/gm-haproxy-stats/issues"
  },
  "homepage": "https://github.com/jcware/gm-haproxy-stats#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.3.2",
    "babel-register": "^6.24.0",
    "chai": "^3.5.0",
    "eslint": "^4.0.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "gulp": "^3.9.1",
    "husky": "^0.11.9",
    "mocha": "^3.2.0",
    "node-fetch": "^1.7.3",
    "nyc": "^10.2.0",
    "rimraf": "^2.6.1",
    "sinon": "^4.1.2",
    "sinon-chai": "^2.14.0",
    "sinon-stub-promise": "^4.0.0"
  }
}
